<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>ged.entrant.web</display-name>
	<servlet>
		<servlet-name>Upload</servlet-name>
		<servlet-class>fr.cimut.ged.entrant.interrogation.servlet.Upload</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SudeUpload</servlet-name>
		<servlet-class>fr.cimut.ged.entrant.interrogation.servlet.SudeFileManager</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BinaryUpload</servlet-name>
		<servlet-class>fr.cimut.ged.entrant.interrogation.servlet.BinaryFileManager</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MulticanalUpload</servlet-name>
		<servlet-class>fr.cimut.ged.entrant.interrogation.servlet.MulticanalFileManager</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Test</servlet-name>
		<servlet-class>fr.cimut.ged.entrant.interrogation.servlet.Test</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Upload</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SudeUpload</servlet-name>
		<url-pattern>/sudeUpload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BinaryUpload</servlet-name>
		<url-pattern>/binaryUpload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MulticanalUpload</servlet-name>
		<url-pattern>/multicanalUpload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>180</session-timeout>
	</session-config>
</web-app>